<%= form_with(model:[score, comment], local: false) do |f| %>
  <%= f.text_area :comment, rows:'5',placeholder: "コメントをここに", class: "w-100 comment-textarea" %>
  <div class="form-group row" id="star">
    <%= f.label :star,'評価', class:'col-md-3 col-form-label' %>
    <%= f.hidden_field :star, id: :review_star %>
  </div>
  <%= f.submit "送信する", class: "btn btn-secondary btn-base-1 mt-20 pull-right" %>
<% end %>